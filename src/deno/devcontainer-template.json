{
    "id": "deno",
    "version": "1.0.0",
    "name": "Deno",
    "description": "A Template for Deno development",
    "documentationURL": "https://github.com/real-ity/devcontainers-templates/tree/main/src/deno",
    "licenseURL": "https://github.com/real-ity/devcontainers-templates/tree/main/LICENSE",
    "options": {
        "denoVersion": {
            "type": "string",
            "description": "Deno Version",
            "proposals": [
                "2.0.0"
            ],
            "default": "2.0.0"
        },
        "nodeVersion": {
            "type": "string",
            "description": "Node Version",
            "proposals": [
                "18",
                "20"
            ],
            "default": "20"
        }
    },
    "platforms": ["Any"]
}
